{
    "RedisNetwork": "tcp",
    "RedisAddress": "127.0.0.1:6379",
    "RedisQueueKey": "taskqueue",
    "Tasks": [
        {
            "Type": "something",
            "Script": "/something.php",
            "Workers": 2
        }
    ],
    "Lockfile": "run.lock",
    "ErrorCmd": "(echo 'Subject: Taskqueue Error'; echo %s) | sendmail mail@example.com"
}
